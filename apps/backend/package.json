{
  "name": "@fixwell/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Backend for Fixwell Services Platform (Express, Prisma)",
  "scripts": {
    "dev": "tsx watch src/app.js",
    "dev:js": "nodemon src/app.js",
    "build": "npm run build:prisma && npm run build:ts",
    "build:prisma": "npx prisma generate",
    "build:ts": "tsc",
    "start": "node dist/app.js",
    "start:js": "node src/app.js",
    "setup": "node setup-database.js",
    "seed": "node prisma/seed.js",
    "migrate": "npx prisma migrate deploy",
    "migrate:dev": "npx prisma migrate dev",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "postinstall": "node -e \"process.env.PRISMA_CLIENT_ENGINE_TYPE='binary'; process.env.PRISMA_ENGINES_DIRECTORY=(require('os').tmpdir() + require('path').sep + 'prisma-engines'); require('child_process').spawnSync('npx', ['prisma','generate'], { stdio:'inherit', shell:true });\""
  },
  "dependencies": {
    "@azure/msal-node": "^3.7.4",
    "@fixwell/types": "file:../../packages/types",
    "@fixwell/utils": "file:../../packages/utils",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@prisma/client": "^6.11.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^3.0.0",
    "dotenv": "^17.0.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.2.1",
    "googleapis": "^160.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "isomorphic-dompurify": "^2.28.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.1",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "socket.io": "^4.7.4",
    "stripe": "^18.3.0",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1",
    "validator": "^13.12.0",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.11.7",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.17.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.11.1",
    "supertest": "^7.1.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
