{
  "name": "@fixwell/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Backend for Fixwell Services Platform (Express, Prisma)",
  "scripts": {
    "dev": "nodemon src/app.js",
    "build": "npx prisma generate",
    "start": "node src/app.js",
    "setup": "node setup-mongodb.js",
    "seed": "node prisma/seed.js",
    "migrate": "npx prisma migrate deploy",
    "migrate:dev": "npx prisma migrate dev",
    "lint": "eslint .",
    "type-check": "tsc --noEmit || echo 'No TS config yet'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@fixwell/types": "file:../../packages/types",
    "@fixwell/utils": "file:../../packages/utils",
    "@prisma/client": "^6.11.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.1",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "socket.io": "^4.7.4",
    "stripe": "^18.3.0",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.17.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.11.1",
    "supertest": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
