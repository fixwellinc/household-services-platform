{
  "buildCommand": "npm run build:vercel",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "true",
    "SKIP_ENV_VALIDATION": "true",
    "NEXT_PUBLIC_API_URL": "https://cultured-account-production.up.railway.app/api"
  }
} 